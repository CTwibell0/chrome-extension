{
    "name": "Get away from desk Event Popup",
    "description": "Demonstrates usage and features of the event page by reminding user to drink water",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "tabs",
        "alarms",
        "notifications",
        "storage"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_title": "Get up Event",
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "desk_sitting_16.png",
        "32": "desk_sitting_32.png",
        "48": "desk_sitting_16.png",
        "128": "desk_sitting_16.png"
    }
}